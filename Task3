using System;

class Task3
{
    static void PrintNumberTriangle(int rows)
    {
        for (int i = 1; i <= rows; i++)
        {
            for (int j = 1; j <= i; j++)
                Console.Write(i + " ");
            Console.WriteLine();
        }
    }

    static void Main()
    {
        Console.Write("Введите количество строк для треугольника: ");
        int rows = int.Parse(Console.ReadLine());
        PrintNumberTriangle(rows);
    }
}
